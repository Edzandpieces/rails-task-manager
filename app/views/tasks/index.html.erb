<div class="container">
  <h1>My tasks</h1>
    <ul>
      <% @tasks.each do |task|%>
      <li><%= link_to task.title, task_path(task) %></li>
        <ul>
          <li><%= task.id %></li>
          <li><%= task.details %></li>
          <li><%= task.completed %></li>
        </ul>
      <% end %>
    </ul>
    <%= link_to "Create a new task", new_task_path %>
</div>
